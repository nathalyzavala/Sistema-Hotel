<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="hotel" match-schema="public"/>
  <table-filter match-name="cargos"/>
  <table-filter match-name="categoria"/>
  <table-filter match-name="consumo"/>
  <table-filter match-name="clientes"/>
  <table-filter match-name="usuarios"/>
  <table-filter match-name="pago"/>
  <table-filter match-name="habitacion"/>
  <table-filter match-name="producto"/>
  <table-filter match-name="reserva"/>
  <table-filter match-name="empleado"/>
</hibernate-reverse-engineering>
